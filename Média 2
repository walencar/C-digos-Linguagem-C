#include <stdio.h>
#include <stdlib.h>
 
int main() {
  
    double peso1=2, peso2=3, peso3=5, A, B, C, MEDIA;
     
    scanf ("%lf", &A);
    scanf ("%lf", &B);
    scanf ("%lf", &C);
     
MEDIA = ((peso1*A)+(peso2*B)+(peso3*C))/10;    
     
 printf("MEDIA = %.1lf\n",MEDIA);
  
 system ("pause");
  
    return 0;
     
}
